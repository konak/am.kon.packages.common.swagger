<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netstandard2.1</TargetFramework>
    <Nullable>disable</Nullable>
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
    <PackageId>am.kon.packages.common.swagger</PackageId>
    <PackageVersion>1.0.0.0</PackageVersion>
    <Authors>Konakov Sergey</Authors>
    <Description>Package containing extensions, used for Swagger configuration</Description>
    <RepositoryUrl>https://github.com/konak/am.kon.packages.common.swagger</RepositoryUrl>
    <RepositoryType>git</RepositoryType>
    <PackageTags>konak packages swagger</PackageTags>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="Swashbuckle.AspNetCore" Version="6.5.0" />
  </ItemGroup>
</Project>
